---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Mark Hansen</title>
 <link href="http://markhansen.co.nz/atom.xml" rel="self"/>
 <link href="http://markhansen.co.nz/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://markhansen.co.nz/</id>
 <author>
   <name>Mark Hansen</name>
   <email>mark@markhansen.co.nz</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://markhansen.co.nz{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://markhansen.co.nz{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
